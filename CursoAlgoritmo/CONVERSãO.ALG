Algoritmo "Conversão"

Var
   RS: Real

Procedimento Conversao(var R: Real)
var S: Real
Inicio

     S <- (3.45 * R)
      Escreva("Convertendo...", " Você terá ", "US$",S, " Dolares.")

FimProcedimento

inicio
      EscrevaL("Você tem quantos reais na carteira? ")
      Leia(RS)
      Conversao(RS)
Fimalgoritmo